### Docker ALIASES ###
alias db='docker build -t'
alias di='docker images'
alias dh='web_search dockerhub'
alias drm='docker rm -f $(docker ps -aq)'
alias drmi='docker rmi -f $(docker images -q) && docker images'
alias dstp='docker stop $(docker ps -aq)'
alias dps='docker ps'
alias dpa='docker ps -a'
alias dr='docker run'
alias drit='docker run -it'
alias dt='docker tag'
alias dx='docker exec -it'
alias df='docker logs $(docker ps -a'
alias dl='docker logs -f $(docker ps -a | awk '\''NR>1 {print $NF}'\'' | fzf --preview="" --tmux --border=rounded --prompt="Select docker container: " --layout=reverse)'
